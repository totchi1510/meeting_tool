# Slack (Socket Mode)
SLACK_SIGNING_SECRET=
SLACK_BOT_TOKEN=
SLACK_APP_TOKEN=

# PostgreSQL (Neon/Supabase 等)
DATABASE_URL=

# Google Calendar（OAuth連携で任意指定。未設定ならprimary）
GCALENDAR_ID_SHARED=

# ICS 公開用ベースURL（例: https://meeting-tools.fly.dev）
PUBLIC_BASE_URL=

# Google OAuth（Workspace不要での直接登録用: 有効化で使用）
ENABLE_GCAL_OAUTH=
GOOGLE_OAUTH_CLIENT_ID=
GOOGLE_OAUTH_CLIENT_SECRET=
OAUTH_REDIRECT_BASE_URL=

# App
NODE_ENV=production
APP_TIMEZONE=Asia/Tokyo
